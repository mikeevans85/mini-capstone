<% @quilts.each do |quilt| %>
  <h1><%= quilt.name %></h1>
  <h4><%= quilt.price %></h4>
  <h4><%= quilt.size %></h4>
  <p><%= quilt.description %></p>
  <p><%= quilt.tax %></p>
  <p><%= quilt.total %></p>
  <p><%= quilt.supplier.name %></p>
  <img src="<%= quilt.image_url_list %>">
  <a href="/quilts/<%= quilt.id %>"><button>Quilt Info</button></a>
<% end %>